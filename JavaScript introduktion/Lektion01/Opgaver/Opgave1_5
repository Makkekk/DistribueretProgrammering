function erPrimtal(n) {
    if (n < 2)
    return false;
    for (let i = 2; i <= Math.sqrt(n); i++) {
        if (n % i == 0) {
            return false
        }

    }
    return true;
}

function printPrimTalOpTil(max) {
    for (let i = 2; i <= max; i++) {
        if (erPrimtal(i)) {
            console.log(i);
        }
    }
}

printPrimTalOpTil(20);





